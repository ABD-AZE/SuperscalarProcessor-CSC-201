    %vpi_call 2 89 "$display", "Stalled, instr1: %h, instr2: %h", v0x6344b75c6870_0, v0x6344b75c6970_0 {0 0 0};
    %end;
S_0x6344b7597310 .scope task, "test_with_branch" "test_with_branch" 2 65, 2 65 0, S_0x6344b75a8b00;
 .timescale 0 0;
