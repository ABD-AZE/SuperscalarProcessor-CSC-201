    %end;
S_0x6344b7597130 .scope task, "test_stall" "test_stall" 2 79, 2 79 0, S_0x6344b75a8b00;
 .timescale 0 0;
TD_tb_fetch_unit.test_stall ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6344b75c6bb0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6344b75c6a40_0, 0, 1;
