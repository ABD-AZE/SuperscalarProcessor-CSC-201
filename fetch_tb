TD_tb_fetch_unit.test_no_branch ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6344b75c6a40_0, 0, 1;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x6344b75c66c0_0, 0, 16;
